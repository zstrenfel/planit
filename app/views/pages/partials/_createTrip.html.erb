<div class="create-edit-trip_container modal">
	<a href="" class="right close" data-function="create-trip-close">Close</a>
	<div class="create-edit-trip">
		<div class="block center modal-title">
			<h2>Create Trip</h2>
		</div>
		<div class="form-wrapper center">
			<input type="text" name="location" placeholder ="Location">
			<input type="text" name="name" placeholder="Trip Name">
			<div class="time-container">
                <input type="text" name="start-date" placeholder = "Start Date" id="trip-start" class="time-input">
                <span class="left" style="padding: 5px 10px 0">to</span>
                <input type="text" name="end-date" placeholder = "End Date" id="trip-end" class="time-input">
            </div>
		</div>
		<div class="block center">
			<button class="full-width" data-function="save-trip">Create Trip</button>
		</div>
	</div>

</div>