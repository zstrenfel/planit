<%= javascript_tag do %>
	$(window).load(function() {
	  	Dashboard.start();
	  	$('#dest-date').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('#trip-start').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('#trip-end').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('input[data-function="update-trip-start"').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('input[data-function="update-trip-end"').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('.start-time-input').timepicker();
	  	$('.end-time-input').timepicker();
  	});
<% end %>
