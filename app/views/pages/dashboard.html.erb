<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false"></script>
<%= javascript_tag do %>
	$(window).load(function() {
	  	Dashboard.start();
  	});
<% end %>
<!-- offscreen -->
<div class="invite-friends">
	<h2>Invite Friends</h2>
	<i>Please separate each email by a comma; existing users will be added automatically</i>
	<textarea rows="4" cols="30" data-function="submit-friends">
	</textarea>
	<div class="half-col">
		<a href="" class="invite left">submit</a>
	</div>
	<div class="half-col">
		<a href="" class="close right">close</a>
	</div>
</div>
<div class="create-edit-trip">
	<h2>Create Trip</h2>
	<div class="divider-line"></div>
	<form action="" class="trip-form">
		<input type="text" name="location" placeholder ="Location">
		<input type="text" name="name" placeholder="Trip Name">
		<input type="text" name="start-date" placeholder = "Start Date">
		<input type="text" name="end-date" placeholder = "End Date">
		<a href="" data-function="save-trip">submit</a>
		<a href="" class="right" data-function="create-trip-close">Close</a>
	</form>

</div>
<!--  -->


<div class="menu-base content center">
	<ul>
		<li><a href="" class="trips">Trips</a></li>
	</ul>
</div>
<div class="menu-open">
	<%= render "pages/partials/menu" %>
</div>
<div class="main-content">
	<div class="trip-info">
		<div class="half-col">
			<h1 data-header="location">No Trip Selected</h1>
			<aside data-header="invited-dates"></aside>
			<aside><a href="" data-function="invite-friends" class="hidden">Invite more friends</a></aside>
		</div>
		<a href="" class="button hidden" data-function="form-save">SAVE</a>
	</div>

	<div class="destination-list">
		<h1>Destinations</h1>
		<table id="destTable">
		 <tr class="table-initial">
		    <td>Name:</td>
		    <td>Address:</td>
		    <td>Edit:</td>
		    <td>Delete:</td>
		    <!-- <td>Delete:</td> -->
		 </tr>
		</table>

	</div>

	<div class="submit">

		<h1>Create Destination</h1>
		<form method="get" action="index.html" class="form">
                        <input type="text" placeholder="Name" class="name-input" name="name" />
                        <input type="text" placeholder="Address" class="address-input" name="address" />
                        <input type="submit" class="submit-input my-button" id="create-button"></input>
	</div>

	<div class="edit_form hide">

		<h1>Edit Destination</h1>

		<form method="get" action="index.html" class="form">
                        <input type="text" placeholder="Date" class="date-input" name="date" />
                        <input type="text" placeholder="Time" class="time-input" name="time" />
                        <input type="text" placeholder="Duration" class="duration-input" name="duration" />
                        <input type="submit" class="submit-input my-button" id="create-button"></input>



	</div>


	<div class="search">
		<h1>Search</h1>
		<div id="myDiv"></div>
		<div class="actions">
		    <div class="button">
		        <label for="gmap_where">Where:</label>
		        <input id="gmap_where" type="text" name="gmap_where" /></div>
		    <div id="button2" class="button" onclick="findAddress(); return false;">Search for address</div>
		    <div class="button">
		        <label for="gmap_keyword">Keyword (optional):</label>
		        <input id="gmap_keyword" type="text" name="gmap_keyword" /></div>
		    <div class="button">
		        <label for="gmap_type">Type:</label>
		        <select id="gmap_type">
		            <option value="art_gallery">art_gallery</option>
		            <option value="bar">bar</option>
		            <option value="cafe">cafe</option>
		            <option value="food">food</option>
		            <option value="store">store</option>
		        </select>
		    </div>
		    <input type="hidden" id="lat" name="lat" value="40.7143528" />
		    <input type="hidden" id="lng" name="lng" value="-74.0059731" />
		    <!-- <div type ="submit" class="search-button button">Search for objects</div> -->
		    <input type="submit" class="search-button my-button" id="create-button"></input>
	</div>
		    <table id="destList">
		    	<tr>
		    		<td> Name </td>
		    		<td> Address </td>
		    		<td> Rating </td>
		    		<td class="add">Add</td>
		    	</tr>
		    </table>
		</div>

	<div id="Map" class="map" style="width: 800px; height: 300px;">
	</div>

