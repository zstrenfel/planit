<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false"></script>
<%= javascript_tag do %>
	$(window).load(function() {
	  	Dashboard.start();
	  	$('#dest-date').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('#trip-start').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('#trip-end').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('input[data-function="update-trip-start"').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('input[data-function="update-trip-end"').datepicker({ dateFormat: 'dd-mm-yy' });
	  	$('.start-time-input').timepicker();
	  	$('.end-time-input').timepicker();
  	});
<% end %>
<!-- offscreen -->
<%= render "pages/partials/inviteFriends" %>
<%= render "pages/partials/createTrip" %>
<%= render "pages/partials/editDest" %>
<!--  -->


<div class="menu-base content center">
	<input id="location" type="hidden" data-location="">
	<ul>

		<li class="trip-select">
		<%= inline_svg("icons/aircraft.svg", class: "aircraft-svg") %>
		<a href="" class="trips">Trips</a>
		</li>
	</ul>
</div>
<div class="menu-open">
	<%= render "pages/partials/menu" %>
</div>
<div class="trip-info">
	<div class="half-col">
		<h1 data-header="location" class="">No Trip Selected</h1>
		<div class="half-col-collapse omega">
			<input type="text" class="hidden" data-function="update-trip-location">
		</div>
		<div class="half-col-collapse">
			<input type="text" class="hidden" data-function="update-trip-start">
			<input type="text" class="hidden" data-function="update-trip-end">
		</div>
		<aside data-header="invited-dates"></aside>
		<aside><a href="" data-function="invite-friends" class="hidden">Invite more friends</a></aside>
	</div>
	<a href="" class="button hidden" data-function="form-edit">EDIT</a>
	<a href="" class="button hidden" data-function="trip-delete">DELETE</a>&nbsp;
	<a href="" class="button hidden" data-function="form-submit">SAVE</a>&nbsp;
	<a href="" class="button hidden" data-function="form-cancel">CANCEL</a>
</div>
<div class="main-content hidden">

	<div class="calendar">
		<table class="cal-dates">
			<tr></tr>
		</table>
		<div class="cal-container">
		<table class="cal-dests">
			<tr class="calendar-hours">
			</tr>
			<tr class="dest-row keep" data-row="0"></tr>
		</table>
		</div>
	</div>

	<div id="Map" class="map">
	</div>

	<div class="destination-list center">
		<h1>Destinations</h1>
		<table id="destTable">
		 <tr class="table-initial">

		    <th>Name</th>
		    <th>Address</th>
		    <th class="like-th"></th>
		    <th>Like Count</th>
		    <th></th>
		    <th>Delete</th>


		    <!-- <td>Delete:</td> -->
		 </tr>
		</table>

	</div>

	<div class="dest-form center">
		<div class="dest-manual hidden">
			<h1>Destination (Manual)</h1>
	        <input type="text" placeholder="Name" class="name-input" name="name" />
	        <input type="text" placeholder="Address" class="address-input" name="address" />
	        <input type="submit" class="submit-input my-button" id="create-button"></input>
		</div>
		<div class="search">
			<h1>Search</h1>
			<div id="myDiv"></div>
			<div class="actions">
                <input id="gmap_keyword" type="text" name="gmap_keyword" placeholder="Keyword"/>
                <select id="gmap_type">
			            <option value="art_gallery">Art Gallery</option>
			            <option value="bar">Bar</option>
			            <option value="cafe">Cafe</option>
			            <option value="food">Food</option>
			            <option value="store">Store</option>
		        </select>
	        </div>
			    <input type="hidden" id="lat" name="lat" value="40.7143528" />
			    <input type="hidden" id="lng" name="lng" value="-74.0059731" />
			    <!-- <div type ="submit" class="search-button button">Search for objects</div> -->
			    <input type="submit" class="search-button my-button" id="create-button"></input>
			</div>
		    <table id="destList" class="">
		    	<tr>
		    		<td> Name </td>
		    		<td> Address </td>
		    		<td> Rating </td>
		    		<td class="add">Add</td>
		    	</tr>
		    </table>
		</div>
	</div>






